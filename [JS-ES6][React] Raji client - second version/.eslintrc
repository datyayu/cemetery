{
  "env": { "browser": true },

  "parser": "babel-eslint",

  "plugins": [ "react" ],

  "rules": {
    "arrow-spacing": [2, { "before": true, "after": true }],
    "camelcase": 2,
    "comma-dangle": [2, "never"],
    "comma-spacing": [2, { "before": false, "after": true }],
    "constructor-super": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "eol-last": 2,
    "func-style": [2, "expression"],
    "indent": [2, 2],
    "jsx-quotes": [2, "prefer-double"],
    "newline-after-var": [2, "always"],
    "new-cap": 0,
    "no-alert": 2,
    "no-cond-assign": 1,
    "no-console": 1,
    "no-const-assign": 2,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-regex-spaces": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-process-env": 2,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-with": 2,
    "prefer-arrow-callback": 1,
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "require-jsdoc": 1,
    "semi": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "valid-jsdoc": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "yoda": [2, "never"]
  },

  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "experimentalObjectRestSpread": true,
    "globalReturn": false,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": false,
    "templateString": true,
    "jsx": true
  }
}
